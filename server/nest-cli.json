{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/server/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/server/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/server",
  "projects": {
    "auth-server": {
      "type": "application",
      "root": "apps/auth-server",
      "entryFile": "main",
      "sourceRoot": "apps/auth-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-server/tsconfig.app.json"
      }
    },
    "clients": {
      "type": "library",
      "root": "libs/clients",
      "entryFile": "index",
      "sourceRoot": "libs/clients/src",
      "compilerOptions": {
        "tsConfigPath": "libs/clients/tsconfig.lib.json"
      }
    },
    "conf": {
      "type": "library",
      "root": "libs/conf",
      "entryFile": "index",
      "sourceRoot": "libs/conf/src",
      "compilerOptions": {
        "tsConfigPath": "libs/conf/tsconfig.lib.json"
      }
    },
    "core": {
      "type": "application",
      "root": "apps/core",
      "entryFile": "main",
      "sourceRoot": "apps/core/src",
      "compilerOptions": {
        "tsConfigPath": "apps/core/tsconfig.app.json"
      }
    },
    "dbacc": {
      "type": "library",
      "root": "libs/dbacc",
      "entryFile": "index",
      "sourceRoot": "libs/dbacc/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dbacc/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "mail-server": {
      "type": "application",
      "root": "apps/mail-server",
      "entryFile": "main",
      "sourceRoot": "apps/mail-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mail-server/tsconfig.app.json"
      }
    },
    "notification-server": {
      "type": "application",
      "root": "apps/notification-server",
      "entryFile": "main",
      "sourceRoot": "apps/notification-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-server/tsconfig.app.json"
      }
    },
    "uploader-server": {
      "type": "application",
      "root": "apps/uploader-server",
      "entryFile": "main",
      "sourceRoot": "apps/uploader-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/uploader-server/tsconfig.app.json"
      }
    },
    "ws-server": {
      "type": "application",
      "root": "apps/ws-server",
      "entryFile": "main",
      "sourceRoot": "apps/ws-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ws-server/tsconfig.app.json"
      }
    }
  }
}